<Application x:Class="Kuhar.Lopushok.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Kuhar.Lopushok.Presentation"
             StartupUri="Presentation/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="ColorPrimary">#CFFF</SolidColorBrush>
            <SolidColorBrush x:Key="ColorSecondary">#CEFFF9</SolidColorBrush>
            <SolidColorBrush x:Key="ColorAccent" >#00CC76</SolidColorBrush>
        </ResourceDictionary>
    </Application.Resources>
</Application>
